{
  "name": "@mailchain/encoding",
  "version": "0.31.0",
  "description": "Mailchain encoding tools",
  "license": "Apache-2.0",
  "type": "commonjs",
  "author": {
    "name": "Mailchain",
    "email": "support@mailchain.co",
    "url": "https://mailchain.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mailchain/mailchain-sdk-js.git",
    "directory": "encoding"
  },
  "bugs": {
    "url": "https://github.com/mailchain/mailchain-sdk-js/issues"
  },
  "dependencies": {
    "@noble/hashes": "^1.3.0",
    "@scure/base": "^1.1.1"
  },
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    "./ascii": {
      "require": {
        "types": "./ascii.d.ts",
        "default": "./ascii.js"
      },
      "import": {
        "types": "./ascii.d.mts",
        "default": "./ascii.mjs"
      }
    },
    "./base32": {
      "require": {
        "types": "./base32.d.ts",
        "default": "./base32.js"
      },
      "import": {
        "types": "./base32.d.mts",
        "default": "./base32.mjs"
      }
    },
    "./base58": {
      "require": {
        "types": "./base58.d.ts",
        "default": "./base58.js"
      },
      "import": {
        "types": "./base58.d.mts",
        "default": "./base58.mjs"
      }
    },
    "./base58Check": {
      "require": {
        "types": "./base58Check.d.ts",
        "default": "./base58Check.js"
      },
      "import": {
        "types": "./base58Check.d.mts",
        "default": "./base58Check.mjs"
      }
    },
    "./base64": {
      "require": {
        "types": "./base64.d.ts",
        "default": "./base64.js"
      },
      "import": {
        "types": "./base64.d.mts",
        "default": "./base64.mjs"
      }
    },
    "./consts": {
      "require": {
        "types": "./consts.d.ts",
        "default": "./consts.js"
      },
      "import": {
        "types": "./consts.d.mts",
        "default": "./consts.mjs"
      }
    },
    "./encoding": {
      "require": {
        "types": "./encoding.d.ts",
        "default": "./encoding.js"
      },
      "import": {
        "types": "./encoding.d.mts",
        "default": "./encoding.mjs"
      }
    },
    "./ensure": {
      "require": {
        "types": "./ensure.d.ts",
        "default": "./ensure.js"
      },
      "import": {
        "types": "./ensure.d.mts",
        "default": "./ensure.mjs"
      }
    },
    "./hex": {
      "require": {
        "types": "./hex.d.ts",
        "default": "./hex.js"
      },
      "import": {
        "types": "./hex.d.mts",
        "default": "./hex.mjs"
      }
    },
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./index.d.mts",
        "default": "./index.mjs"
      }
    },
    "./utf8": {
      "require": {
        "types": "./utf8.d.ts",
        "default": "./utf8.js"
      },
      "import": {
        "types": "./utf8.d.mts",
        "default": "./utf8.mjs"
      }
    }
  }
}