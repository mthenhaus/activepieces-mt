{
  "name": "@mailchain/keyring",
  "version": "0.31.0",
  "description": "Mailchain keyring",
  "license": "Apache-2.0",
  "author": {
    "name": "Mailchain",
    "email": "support@mailchain.co",
    "url": "https://mailchain.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mailchain/mailchain-sdk-js.git",
    "directory": "keyring"
  },
  "bugs": {
    "url": "https://github.com/mailchain/mailchain-sdk-js/issues"
  },
  "dependencies": {
    "@mailchain/addressing": "0.31.0",
    "@mailchain/crypto": "0.31.0",
    "@mailchain/encoding": "0.31.0"
  },
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    "./constants": {
      "require": {
        "types": "./constants.d.ts",
        "default": "./constants.js"
      },
      "import": {
        "types": "./constants.d.mts",
        "default": "./constants.mjs"
      }
    },
    "./functions": {
      "require": {
        "types": "./functions.d.ts",
        "default": "./functions.js"
      },
      "import": {
        "types": "./functions.d.mts",
        "default": "./functions.mjs"
      }
    },
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./index.d.mts",
        "default": "./index.mjs"
      }
    },
    "./keyring": {
      "require": {
        "types": "./keyring.d.ts",
        "default": "./keyring.js"
      },
      "import": {
        "types": "./keyring.d.mts",
        "default": "./keyring.mjs"
      }
    }
  }
}