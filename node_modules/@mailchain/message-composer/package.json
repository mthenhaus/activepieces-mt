{
  "name": "@mailchain/message-composer",
  "version": "0.31.0",
  "description": "Mailchain MIME message composer. RFC-5322 compliant",
  "license": "Apache-2.0",
  "author": {
    "name": "Mailchain",
    "email": "support@mailchain.co",
    "url": "https://mailchain.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mailchain/mailchain-sdk-js.git",
    "directory": "message-composer"
  },
  "bugs": {
    "url": "https://github.com/mailchain/mailchain-sdk-js/issues"
  },
  "dependencies": {
    "@mailchain/encoding": "0.31.0",
    "date-fns": "^2.29.2"
  },
  "engines": {
    "node": ">=15.x"
  },
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    "./consts": {
      "require": {
        "types": "./consts.d.ts",
        "default": "./consts.js"
      },
      "import": {
        "types": "./consts.d.mts",
        "default": "./consts.mjs"
      }
    },
    "./contentHandler": {
      "require": {
        "types": "./contentHandler.d.ts",
        "default": "./contentHandler.js"
      },
      "import": {
        "types": "./contentHandler.d.mts",
        "default": "./contentHandler.mjs"
      }
    },
    "./fallback": {
      "require": {
        "types": "./fallback.d.ts",
        "default": "./fallback.js"
      },
      "import": {
        "types": "./fallback.d.mts",
        "default": "./fallback.mjs"
      }
    },
    "./folding": {
      "require": {
        "types": "./folding.d.ts",
        "default": "./folding.js"
      },
      "import": {
        "types": "./folding.d.mts",
        "default": "./folding.mjs"
      }
    },
    "./hasOnlyAscii": {
      "require": {
        "types": "./hasOnlyAscii.d.ts",
        "default": "./hasOnlyAscii.js"
      },
      "import": {
        "types": "./hasOnlyAscii.d.mts",
        "default": "./hasOnlyAscii.mjs"
      }
    },
    "./headerFactories": {
      "require": {
        "types": "./headerFactories.d.ts",
        "default": "./headerFactories.js"
      },
      "import": {
        "types": "./headerFactories.d.mts",
        "default": "./headerFactories.mjs"
      }
    },
    "./headerHandler": {
      "require": {
        "types": "./headerHandler.d.ts",
        "default": "./headerHandler.js"
      },
      "import": {
        "types": "./headerHandler.d.mts",
        "default": "./headerHandler.mjs"
      }
    },
    "./headerOrder": {
      "require": {
        "types": "./headerOrder.d.ts",
        "default": "./headerOrder.js"
      },
      "import": {
        "types": "./headerOrder.d.mts",
        "default": "./headerOrder.mjs"
      }
    },
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./index.d.mts",
        "default": "./index.mjs"
      }
    },
    "./messageComposer": {
      "require": {
        "types": "./messageComposer.d.ts",
        "default": "./messageComposer.js"
      },
      "import": {
        "types": "./messageComposer.d.mts",
        "default": "./messageComposer.mjs"
      }
    },
    "./messageComposerContext": {
      "require": {
        "types": "./messageComposerContext.d.ts",
        "default": "./messageComposerContext.js"
      },
      "import": {
        "types": "./messageComposerContext.d.mts",
        "default": "./messageComposerContext.mjs"
      }
    },
    "./types": {
      "require": {
        "types": "./types.d.ts",
        "default": "./types.js"
      },
      "import": {
        "types": "./types.d.mts",
        "default": "./types.mjs"
      }
    }
  }
}