{
  "name": "@mailchain/sdk",
  "version": "0.31.0",
  "description": "Mailchain sdk for sending messages to web3 addresses",
  "license": "Apache-2.0",
  "author": {
    "name": "Mailchain",
    "email": "support@mailchain.co",
    "url": "https://mailchain.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mailchain/mailchain-sdk-js.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/mailchain/mailchain-sdk-js/issues"
  },
  "homepage": "https://docs.mailchain.com/developer",
  "dependencies": {
    "@mailchain/addressing": "0.31.0",
    "@mailchain/crypto": "0.31.0",
    "@mailchain/encoding": "0.31.0",
    "@mailchain/internal": "0.31.0",
    "@mailchain/keyring": "0.31.0"
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./index.d.mts",
        "default": "./index.mjs"
      }
    },
    "./internal/addressNonce": {
      "require": {
        "types": "./internal/addressNonce.d.ts",
        "default": "./internal/addressNonce.js"
      },
      "import": {
        "types": "./internal/addressNonce.d.mts",
        "default": "./internal/addressNonce.mjs"
      }
    },
    "./internal": {
      "require": {
        "types": "./internal/index.d.ts",
        "default": "./internal/index.js"
      },
      "import": {
        "types": "./internal/index.d.mts",
        "default": "./internal/index.mjs"
      }
    },
    "./internal/keys": {
      "require": {
        "types": "./internal/keys.d.ts",
        "default": "./internal/keys.js"
      },
      "import": {
        "types": "./internal/keys.d.mts",
        "default": "./internal/keys.mjs"
      }
    },
    "./internal/privateMessagingKey": {
      "require": {
        "types": "./internal/privateMessagingKey.d.ts",
        "default": "./internal/privateMessagingKey.js"
      },
      "import": {
        "types": "./internal/privateMessagingKey.d.mts",
        "default": "./internal/privateMessagingKey.mjs"
      }
    },
    "./internal/resolvers": {
      "require": {
        "types": "./internal/resolvers.d.ts",
        "default": "./internal/resolvers.js"
      },
      "import": {
        "types": "./internal/resolvers.d.mts",
        "default": "./internal/resolvers.mjs"
      }
    },
    "./mailchain": {
      "require": {
        "types": "./mailchain.d.ts",
        "default": "./mailchain.js"
      },
      "import": {
        "types": "./mailchain.d.mts",
        "default": "./mailchain.mjs"
      }
    },
    "./types": {
      "require": {
        "types": "./types.d.ts",
        "default": "./types.js"
      },
      "import": {
        "types": "./types.d.mts",
        "default": "./types.mjs"
      }
    }
  }
}